import java.util.Scanner;

public class Surya {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("enter a number:");
        int num= input.nextInt();
        while(num<=100){
            System.out.print("enter a number:");
            int i = input.nextInt();
            num = num+i;
        }
        System.out.println("Done");
    }
}
