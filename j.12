import java.util.Scanner;

public class Aa {
    public static void main(String[] args) {
        Scanner input=new Scanner(System.in);
        while(true){
            System.out.print("Enter a number between 1 and 10:");
            int n= input.nextInt();
            if(n<0 || n>10)
                continue;
            System.out.println(n+" is a number 1 and  10.");
        }
    }
}
