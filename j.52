public class FIrst {
    public static void main(String[] args){
        int[] numbers={1,2};
        change(numbers);
        printArray(numbers);
    }
    public static void change(int[] num){
        num[0]=2;
        num[1]=1;
    }
    public static void printArray(int[] number){
        for(int i=0;i< number.length;i++){
            System.out.println(number[i]);
        }
    }
}
