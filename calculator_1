import java.util.Scanner;

public class Surya {
    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        System.out.print("enter the number_1 operator number_2:");
        int a = input.nextInt();
        String b = input.next();
        int c = input.nextInt();
        if (b.equals("+"))
            System.out.println((a + c));
        else if (b.equals("-"))
            System.out.println((a - c));
        else if (b.equals("*"))
            System.out.println((a * c));
        else
            System.out.println((a / c));
    }
}
